{
  "name": "halo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node -r source-map-support/register build/server.js",
    "watch-node": "nodemon --watch build/server.js build/server.js",
    "watch-ts": "tsc -w",
    "watch-parcel": "parcel watch ./src/entries/*",
    "build-parcel": "parcel build ./src/entries/*",
    "build-ts": "tsc",
    "build": "concurrently npm:build-ts npm:build-parcel",
    "clean": "rm -rf ./dist",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node,Parcel\" npm:watch-ts npm:watch-node npm:watch-parcel"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/express-fileupload": "^1.1.3",
    "@types/node": "^13.11.1",
    "@types/passport": "^1.0.3",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-session": "^1.17.1",
    "nodemon": "^2.0.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "sass": "^1.26.3",
    "source-map-support": "^0.5.19",
    "sqlite3": "^4.1.1",
    "tmp": "^0.1.0",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "typescript": "^3.8.3"
  }
}
