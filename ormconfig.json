{
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": false,
   "logging": false,
   "entities": [
      "build/models/entity/**/*.js"
   ],
   "migrations": [
      "build/models/migration/**/*.js"
   ],
   "subscribers": [
      "build/models/subscriber/**/*.js"
   ],
   "cli": {
      "entitiesDir": "src/models/entity",
      "migrationsDir": "src/models/migration",
      "subscribersDir": "src/models/subscriber"
   }
}