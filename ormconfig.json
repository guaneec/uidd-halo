{
   "type": "sqlite",
   "database": "database.sqlite",
   "synchronize": false,
   "logging": false,
   "entities": [
      "models/entity/**/*.ts"
   ],
   "migrations": [
      "models/migration/**/*.ts"
   ],
   "subscribers": [
      "models/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "models/entity",
      "migrationsDir": "models/migration",
      "subscribersDir": "models/subscriber"
   }
}
